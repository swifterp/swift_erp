<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 다른 mapper와 중복되지 않도록 네임스페이스 기재 -->
<mapper namespace="resources.mapper.selectListMapper">
	<insert id="insertMem" parameterType="java.util.HashMap">
		insert into member values(
			#{memEmail}, #{memPw}, #{memName})
    </insert>

    <select id="selectMem" parameterType="String" resultType="java.util.HashMap">
        select mem_name from member where mem_email = #{memEmail}
    </select>
</mapper>
