<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="resources.mapper.selectListMapper">

	<select id="getBusinessLogList" resultType="hashmap">
	
		select * from Business_Log
	
	</select>

	<select id="getMonthWorkList" resultType="hashmap">
	
		select attend_date,empno,attend_open,attend_close from attend_manager_t
	
	</select>
	
	<select id="getWorkStateList" resultType="hashmap">
	
		select attend_date, empno, attend_code from attend_manager_t
	
	</select>
	
	<select id="getWorkTimeList" resultType="hashmap">
	
		select attend_date, empno, attend_time from attend_manager_t
	
	</select>
	
	<select id="getWorkEtcList" resultType="hashmap">
	
		select * from attend_manager_t
	
	</select>

</mapper>